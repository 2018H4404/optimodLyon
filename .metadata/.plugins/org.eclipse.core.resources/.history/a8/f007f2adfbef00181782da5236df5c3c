package services;

import java.util.Collection;
import java.util.HashMap;

import modele.Entrepot;
import modele.Intersection;

public class EntrepotFactory {
	private static HashMap<Long,Entrepot> entrepots;
	
	public static void initEntrepotFactory() {
		entrepots = new HashMap<Long,Entrepot>();
	}
	
	public static void ajouterEntrepot(Intersection unInter,int heure,int minute,int seconde) {
		Entrepot tempObject = new Entrepot(unInter,heure,minute,seconde);
		entrepots.put(unInter.getId(),tempObject);
	}
	
	public static void clear() {
		entrepots.clear();
	}
}
