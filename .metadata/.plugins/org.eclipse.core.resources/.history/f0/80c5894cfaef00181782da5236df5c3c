package modele;

import org.joda.time.DateTime;

public class Entrepot extends Intersection{
	protected DateTime heureDepart;
	protected DateTime heureArrivee;
	
	public Entrepot(long unId, int h, int m, int s) {
		this.idAdresse = unId;
		this.heureDepart = new DateTime(2018,11,30,heure,minute,seconde);
		this.heureArrivee = null;
	}

	@Override
	public String toString() {
		return "Entrepot [idAdresse=" + idAdresse + ", heureDepart=" + heureDepart + ", heureArrivee=" + heureArrivee
				+ "]";
	}

	
	
	
}
