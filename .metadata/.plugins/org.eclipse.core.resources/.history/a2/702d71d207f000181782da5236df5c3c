package modele;

import java.util.Observable;

import org.joda.time.DateTime;

public class Entrepot extends Observable{
	private Intersection monInter;
	private DateTime heureDepart;
	private DateTime heureArrivee;
	
	public Entrepot(Intersection unInter, int heure, int minute, int seconde) {
		this.monInter = unInter;
		this.heureDepart = new DateTime(2018,11,30,heure,minute,seconde);
		this.heureArrivee = null;
	}

	@Override
	public String toString() {
		return "Entrepot [monInter=" + monInter + ", heureDepart=" + heureDepart + ", heureArrivee=" + heureArrivee
				+ "]";
	}

	

	
	
	
}
