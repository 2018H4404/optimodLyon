package modele;

import org.joda.time.DateTime;

public class Entrepot{
	private Intersection monInter;
	private DateTime heureDepart;
	private DateTime heureArrivee;
	
	public Entrepot(long unId, int heure, int minute, int seconde) {
		this.id = unId;
		this.heureDepart = new DateTime(2018,11,30,heure,minute,seconde);
		this.heureArrivee = null;
	}

	@Override
	public String toString() {
		return "Entrepot [idAdresse=" + idAdresse + ", heureDepart=" + heureDepart + ", heureArrivee=" + heureArrivee
				+ "]";
	}

	
	
	
}
